<script>
    var photos = new Array();
    $(document).ready(function () {
        $(".mod-photo").each(function () {
            var $o = $(this);

            var mediaItemID, caption;
            if ($o.find(".mod-config").length > 0) {
                var data = JSON.parse($(this).find(".mod-config").html().replace("/\n/gi", ""));
                caption = data.Caption;
                mediaItemID = data.MediaItemID;
            }
            else {
                mediaItemID = $o.find('img').data('mediaitemid');
                if (mediaItemID) {
                    if ($o.find('.mod-photo-credit').length > 0) {
                        caption = $o.find('.mod-photo-caption').text() + "<div class='photo-credit'>photo credit: " + $o.find('.mod-photo-credit').text() + "</div>";
                    } 
                    else {
                        caption = $o.find('.mod-photo-caption').text();
                    }
                }
                else {
                    caption = $o.data("caption");
                    mediaItemID = $o.data("mediaitemid");
                }
            }

            $o.click(function () {
                std.previewPhoto(mediaItemID, caption, $o.find('img').attr('src'));
            });
        });
    });
</script>

